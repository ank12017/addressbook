<%= form_for @entry, html: {multipart: true}, :remote => true do |form| %>
   <div class="field">
    <%= form.label :name,  class:'control-label' %>
    <%= form.text_field :name, id: :entry_name, class: 'form-control' %>
  </div>

  <div class="field">
    <%= form.label :address,  class: 'control-label' %>
    <%= form.text_field :address, id: :entry_address, class: 'form-control' %>
  </div>

  <div class="field">
    <%= form.label :phone,  class: 'control-label' %>
    <%= form.text_field :phone, id: :entry_phone, class: 'form-control' %>
  </div>

  <div class="field">
    <%= form.label :email,  class: 'control-label' %>
    <%= form.text_field :email, id: :entry_email, class: 'form-control' %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
